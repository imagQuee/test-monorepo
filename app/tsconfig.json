{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "types": [
            "node",
            "jest",
            "@testing-library/jest-dom"
        ],
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "src/**/*"
, "src/components/CustomElement/Calendar"    ],
